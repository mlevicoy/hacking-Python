    "# Se va a leer un json\n",
    "import json\n",
    "# Se va a consultar una web\n",
    "import urllib.request\n",
    "\n",
    "def main():\n",
    "    objetivo = urllib.request.urlopen(\"http://bogado.cl/wp-json/wp/users\")\n",
    "    for x in json.loads(objetivo.read()):\n",
    "        usuario = x['slug']\n",
    "    print(usuario)\n",
